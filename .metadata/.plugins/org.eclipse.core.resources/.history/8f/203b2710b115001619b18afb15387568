package ch.puki.rmca.exceptions;

 /**
 * RobotSamplingException
 *
 * @author pum
 * @author $Author$
 * @version $Rev$
 */
public class RobotSamplingException extends Exception{

    public RobotSamplingException(String message) {
        super(message);
    }

    public RobotSamplingException(String message, Throwable cause) {
        super(message, cause);
    }
    
    

}
